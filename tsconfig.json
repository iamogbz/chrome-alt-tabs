{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./src",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "outDir": "./built",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "target": "es6"
    },
    "exclude": ["node_modules"]
}
